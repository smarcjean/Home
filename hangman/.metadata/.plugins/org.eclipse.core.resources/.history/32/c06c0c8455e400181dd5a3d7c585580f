import java.util.*;
public class pick2 {
	
	public static void main(String[] args) {
	
		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j<= i; j++) {
				System.out.println(" ");
			}
		}

		for (int i = 1; i <= (11 - 2); i++) {
			System.out.println("*");
		}
		
		System.out.println();		
		
		
		
		
	}

}
